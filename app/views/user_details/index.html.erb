<h1>Listing user_details</h1>

<table>
  <tr>
    <th>Keyword</th>
    <th>Keyword tweet</th>
    <th>Name</th>
    <th>Location</th>
    <th>User created at</th>
    <th>Profile image url</th>
    <th>Web url</th>
    <th>Followers</th>
    <th>Following</th>
    <th>Description</th>
    <th>Screen name</th>
  </tr>

<% @user_details.each do |user_detail| %>
  <tr>
    <td><%=h user_detail.keyword_id %></td>
    <td><%=h user_detail.keyword_tweet_id %></td>
    <td><%=h user_detail.name %></td>
    <td><%=h user_detail.location %></td>
    <td><%=h user_detail.user_created_at %></td>
    <td><%=h user_detail.profile_image_url %></td>
    <td><%=h user_detail.web_url %></td>
    <td><%=h user_detail.followers %></td>
    <td><%=h user_detail.following %></td>
    <td><%=h user_detail.description %></td>
    <td><%=h user_detail.screen_name %></td>
    <td><%= link_to 'Show', user_detail %></td>
    <td><%= link_to 'Edit', edit_user_detail_path(user_detail) %></td>
    <td><%= link_to 'Destroy', user_detail, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user_detail', new_user_detail_path %>